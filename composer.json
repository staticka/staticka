{
  "name": "staticka/staticka",
  "description": "Yet another PHP-based static site generator.",
  "keywords": [ "simple", "static-site", "generator" ],
  "homepage": "https://roug.in/staticka",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "homepage": "https://roug.in/",
      "name": "Rougin Gutib",
      "role": "Software Engineer"
    }
  ],
  "require":
  {
    "php": ">=5.3.0",
    "rougin/staticka": "dev-master"
  },
  "require-dev":
  {
    "phpunit/phpunit": "~4.2|~5.7|~6.0|~7.0|~8.0|~9.0",
    "sanmai/phpunit-legacy-adapter": "~6.1|~8.0"
  },
  "autoload":
  {
    "psr-4":
    {
      "Staticka\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Staticka\\": "tests"
    }
  },
  "scripts":
  {
    "test": "phpunit"
  }
}