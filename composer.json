{
    "name": "staticka/staticka",
    "description": "A simple and extensible PHP static site generator.",
    "keywords": ["static site", "php library", "staticka"],
    "homepage": "https://github.com/staticka/staticka",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "erusev/parsedown": "~1.6",
        "zapheus/zapheus": "~0.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Staticka\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staticka\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}