{
    "name": "staticka/staticka",
    "description": "A simple yet extensible \"do-it-yourself\" static site generator.",
    "keywords": ["do-it-yourself", "generator", "simple", "static site", "staticka"],
    "homepage": "https://staticka.github.io/",
    "license": "MIT",
    "authors":
    [
        {
            "email": "rougingutib@gmail.com",
            "homepage": "https://roug.in/",
            "name": "Rougin Gutib",
            "role": "Software Engineer"
        }
    ],
    "require":
    {
        "php": ">=5.3.0",
        "erusev/parsedown": "~1.6",
        "symfony/yaml": ">=2.8",
        "twig/twig": "~1.3",
        "zapheus/zapheus": "~0.1"
    },
    "require-dev":
    {
        "phpunit/phpunit": "~4.2|~5.7",
        "scrutinizer/ocular": "~1.1.0"
    },
    "autoload":
    {
        "psr-4":
        {
            "Staticka\\": "src"
        }
    },
    "autoload-dev":
    {
        "psr-4":
        {
            "Staticka\\": "tests"
        }
    },
    "scripts":
    {
        "test": "phpunit"
    },
    "extra":
    {
        "branch-alias":
        {
            "dev-master": "1.0-dev"
        }
    }
}