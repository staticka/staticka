{
    "name": "rougin/staticka",
    "description": "Yet Another PHP Static Site Generator",
    "keywords": [ "static site", "static cms", "php library", "staticka" ],
    "homepage": "https://github.com/rougin/staticka",
    "license": "MIT",
    "authors": [
        {
            "name": "Rougin Royce Gutib",
            "email": "rougingutib@gmail.com",
            "homepage": "https://rougin.github.io",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "erusev/parsedown": "~1.6",
        "illuminate/view": "~4.2 || ~5.0",
        "nesbot/carbon": "~1.2",
        "rougin/slytherin": "~0.9",
        "rougin/weasley": "~0.5",
        "symfony/console": "~2.8"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.2",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "Rougin\\Staticka\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Rougin\\Staticka\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
