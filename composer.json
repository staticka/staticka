{
  "name": "staticka/staticka",
  "description": "Yet another PHP-based static site generator.",
  "keywords": [ "simple", "static-site", "site-generator" ],
  "homepage": "https://roug.in/staticka/",
  "license": "MIT",
  "authors":
  [
    {
      "email": "rougingutib@gmail.com",
      "homepage": "https://roug.in/",
      "name": "Rougin Gutib",
      "role": "Software Engineer"
    }
  ],
  "require":
  {
    "php": ">=5.3.0",
    "erusev/parsedown": "~1.6",
    "rougin/slytherin": "~0.9",
    "symfony/yaml": "~2.8|~3.0|~4.0|~5.0|~6.0|~7.0"
  },
  "require-dev":
  {
    "phpunit/phpunit": "~4.2|~5.7|~6.0|~7.0|~8.0|~9.0",
    "sanmai/phpunit-legacy-adapter": "~6.1|~8.0"
  },
  "autoload":
  {
    "psr-4":
    {
      "Staticka\\": "src"
    }
  },
  "autoload-dev":
  {
    "psr-4":
    {
      "Staticka\\": "tests"
    }
  },
  "scripts":
  {
    "test": "phpunit"
  },
  "extra":
  {
    "branch-alias":
    {
      "dev-master": "1.0-dev"
    }
  }
}